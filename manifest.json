{
  "name": "Derpisearch",
  "version": "1.0",
  "description": "Quickly search for artists on derpibooru",
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "permissions": [
    "contextMenus",
    "tabs"
  ],
  "icons": {
    "16": "icon_tiny.png",
    "48": "icon_small.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}